<%= simple_form_for ([@workout, @workout.exercises.build]) do |f| %>
  <%= f.input :name, label: "Name", input_html: { class: 'form-control' } %>
  <%= f.input :sets, label: "Sets", input_html: { class: 'form-control' } %>
  <%= f.input :reps, label: "Reps", input_html: { class: 'form-control' } %>
  <%= f.button :submit %>
  <br>
<% end %>
